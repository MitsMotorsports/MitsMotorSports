{
  "rewrites": [
    {
      "source": "/assets/:path*",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "^(?!image/png).*$"
        }
      ],
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
